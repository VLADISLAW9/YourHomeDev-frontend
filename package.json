    "@siberiacancode/reactuse": "^0.0.72",
    "@tanstack/react-query": "^5.60.5",
    "@tanstack/react-router": "^1.81.10",
    "axios": "^1.7.7",
    "feather-icons": "^4.29.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.2",
    "react-icons": "^5.3.0",
    "react-number-format": "^5.4.2",
    "zod": "^3.23.8"
